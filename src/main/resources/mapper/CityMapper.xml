<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="nu.springboot2.mapper.CityMapper" >

    <select id="findByState" parameterType="String" resultType="nu.springboot2.module.City">
        select * from t_city where state = #{state}
    </select>

</mapper>